<!DOCTYPE html>
<html lang="en">
<head>
    <title>Blog Posts</title>
    <link rel="stylesheet" href="/public/styles.css">


</head>
<body>
    <div class="container">
        <h1>All Blog Posts</h1>
        <ul>
            <% posts.forEach(post => { %>
                <li>
                    <h2><%= post.title %></h2>
                    <p><%= post.content %></p>
                    <a href="/post?id=<%= post.id %>">Read More</a>
                </li>
            <% }); %>
        </ul>
        <a href="/add-post">Add New Post</a>
    </div>
</body>
</html>
